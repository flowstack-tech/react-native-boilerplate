{
  "name": "studyroom",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "clean-android": "cd android && rm -R .gradle && cd app && rm -R build",
    "java": "java -version",
    "resource": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res",
    "build:androiddev": "cd android && ./gradlew app:assembleRelease",
    "build:android:production:debug": "cd android && ./gradlew app:assembleDebug",
    "build:android:production": "cd android && ./gradlew app:assembleRelease",
    "build:android:production:bundle": "cd android &&  ./gradlew bundleRelease",
    "ios:staging": "react-native run-ios",
    "ios:production": "react-native run-ios"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.13.3",
    "@react-native-community/masked-view": "^0.1.10",
    "@react-navigation/native": "^5.9.2",
    "apisauce": "^2.0.1",
    "i18n-js": "^3.8.0",
    "mobx": "^6.1.0",
    "mobx-react-lite": "^3.2.0",
    "mobx-utils": "^5.6.2",
    "react": "16.13.1",
    "react-hook-form": "^6.14.2",
    "react-native": "0.63.4",
    "react-native-gesture-handler": "^1.9.0",
    "react-native-paper": "^4.7.1",
    "react-native-reanimated": "^1.13.2",
    "react-native-safe-area-context": "^3.1.9",
    "react-native-screens": "^2.17.1",
    "react-native-vector-icons": "^8.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/runtime": "^7.8.4",
    "@react-native-community/eslint-config": "^1.1.0",
    "@types/i18n-js": "^3.8.0",
    "@types/jest": "^25.2.3",
    "@types/react-native": "^0.63.2",
    "@types/react-test-renderer": "^16.9.2",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "babel-jest": "^25.1.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "^6.5.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "jest": "^25.1.0",
    "metro-react-native-babel-preset": "^0.59.0",
    "react-test-renderer": "16.13.1",
    "typescript": "^3.8.3"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
